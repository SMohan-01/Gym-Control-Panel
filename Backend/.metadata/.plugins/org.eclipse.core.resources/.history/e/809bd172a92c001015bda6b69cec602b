package in.mohan.gym_management_system.repository;

import java.time.LocalDate;
import java.util.List;
import java.util.Optional;
import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;
import in.mohan.gym_management_system.entity.MemberEntity;


@Repository
public interface MemberRepository extends JpaRepository<MemberEntity, Long>{
	
	Optional<MemberEntity> findByEmailAddress(String emailAddress);
	
	List<MemberEntity> findByMembershipEndDateBefore(LocalDate date);
	
	List<MemberEntity> findByMembershipEndDateBefore(LocalDate date);
}
